public class Calculator {
    public static void main(String[] args) {
        int a = 7; // modified
        int b = 10;

        int sum = a + b;
        int difference = b - a;
        int product = a * b; // added

        System.out.println("Sum: " + sum);
        System.out.println("Difference: " + difference);
        System.out.println("Product: " + product); // added
    }
}
